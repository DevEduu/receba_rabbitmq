# Configurações do RabbitMQ
# Copie este arquivo para rabbitmq.env e ajuste os valores conforme necessário

RABBITMQ_DEFAULT_USER=admin
RABBITMQ_DEFAULT_PASS=admin123
RABBITMQ_DEFAULT_VHOST=/
RABBITMQ_ERLANG_COOKIE=SWQOKODSQALRPCLNMEQG

# Configurações de rede
RABBITMQ_AMQP_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672
RABBITMQ_CLUSTERING_PORT=25672

# Configurações de recursos
RABBITMQ_DISK_FREE_LIMIT=1GB
RABBITMQ_MEMORY_HIGH_WATERMARK=0.6

# Configurações de log
RABBITMQ_LOG_LEVEL_CONNECTION=error
RABBITMQ_LOG_LEVEL_DEFAULT=info
